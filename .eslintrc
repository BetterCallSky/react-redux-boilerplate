{
  "parser"  : "babel-eslint",
  "extends" : [
    "airbnb"
  ],
  "plugins": [
    "babel"
  ],
  "env"     : {
    "browser" : true,
    "mocha" : true
  },
  "globals" : {
    "__DEV__"      : false,
    "__PROD__"     : false,
    "__DEBUG__"    : false,
    "__BASENAME__" : false
  },
  "rules": {
    "global-require": 0,
    "no-script-url": 0,
    "max-len": 0,
    "new-cap": 0,
    "react/jsx-no-bind": 0,
    "no-mixed-operators": 0,
    "arrow-parens": [2, "always"],
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 0,
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": 0,
    "react/forbid-prop-types": 0,
  }
}
