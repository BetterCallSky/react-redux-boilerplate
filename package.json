{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "React, Redux, Webpack, CSS Modules",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod",
    "stylelint": "stylelint src/**/*.scss",
    "lint": "eslint --ext jsx --ext js .",
    "lint:fix": "npm run lint -- --fix",
    "build": "better-npm-run build",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch"
  },
  "keywords": [
    "react",
    "redux",
    "starter",
    "boilerplate"
  ],
  "bugs": {
    "url": "https://github.com/lsentkiewicz/react-redux-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lsentkiewicz/react-redux-boilerplate.git"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --bail --progress --build --tc",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node server",
      "env": {
        "PORT": 3000,
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "node server",
      "env": {
        "PORT": 3000,
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "karma start karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "test:watch": {
      "command": "karma start karma.conf --watch",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "homepage": "https://github.com/lsentkiewicz/react-redux-boilerplate#readme",
  "author": "≈Åukasz Sentkiewicz",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.4.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.1.1",
    "bluebird": "^3.4.6",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.4",
    "node-sass": "^4.10.0",
    "postcss-flexboxfixer": "0.0.5",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-css-modules": "^4.7.8",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-redux": "^4.0.6",
    "redbox-react": "^1.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-connect": "^8.0.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^0.22.0",
    "connect-history-api-fallback": "^1.3.0",
    "enzyme": "^3.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.14.0",
    "isparta-loader": "^2.0.0",
    "karma": "^3.1.3",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack-with-fast-source-maps": "^1.10.0",
    "mocha": "^5.2.0",
    "phantomjs-prebuilt": "^2.1.13",
    "react-addons-test-utils": "^15.3.2",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.12.2",
    "yargs": "^12.0.5"
  }
}
