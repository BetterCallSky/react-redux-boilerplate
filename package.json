{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "React, Redux, Webpack, CSS Modules",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod",
    "lint": "eslint --ext jsx --ext js .",
    "lint:fix": "npm run lint -- --fix",
    "build": "better-npm-run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "redux",
    "starter",
    "boilerplate"
  ],
  "bugs": {
    "url": "https://github.com/lsentkiewicz/react-redux-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lsentkiewicz/react-redux-boilerplate.git"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --bail --progress --build --tc",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node server",
      "env": {
        "PORT": 3000,
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "node server",
      "env": {
        "PORT": 3000,
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "homepage": "https://github.com/lsentkiewicz/react-redux-boilerplate#readme",
  "author": "≈Åukasz Sentkiewicz",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.11",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "lodash": "^4.16.4",
    "node-sass": "^3.10.1",
    "postcss-flexboxfixer": "0.0.5",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "react": "^15.3.2",
    "react-css-modules": "^3.7.10",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redbox-react": "^1.3.1",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-connect": "^3.0.0",
    "redux-form": "^6.1.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-preset-react-hmre": "^1.1.1",
    "connect-history-api-fallback": "^1.3.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "express": "^4.14.0",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.12.2"
  }
}
